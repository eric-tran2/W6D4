<ul>
  <%@cats.each do |cat|%>
    <li>Birthdate: <%= cat.birth_date %></li>
    <li>Color: <%= cat.color %></li>
    <li>Name: <%= cat.name %></li>
    <li>Sex: <%= cat.sex %></li>
    <li>Age: <%= cat.age %></li>
    <li><a href="<%= cat_url(cat.id) %>">Go to Cat show page</a></li>
  <% end %>
</ul>